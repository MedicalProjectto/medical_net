        //新增用户
        function AddAjaxUser()
        {
            var role= $("input[name='optionsRadiosinline']:checked").val();
            var idcard = $('#user-idcard').val();
            var username = $('#user-name').val();
            var email = $('#user-email').val();
            var mobile = $('#user-mobile').val();

            var deptname = $('#user-deptname').val();
            var duty = $('#user-duty').val();
            var tel = $('#user-tel').val();
            var title = $('#user-hospitalname').val();

            //alert("角色："+role);
            //alert('当前医院ID:'+currentHospitalId);

            if (email) {//邮箱不必填
                if(!regEmail.test(email))
                {
                    alert('请输入正确的邮箱格式！');
                    return;
                }
            }

            if (!username||!idcard||!mobile) {
                alert('请填写完成资料！');
            }
            else if(!regPhone.test(mobile)){
                alert('请输入正确的手机号码！');
            }
            else if(!regIdCard.test(idcard)){
                alert('请输入正确的身份证号码！');
            }

            else{

                //根据身份证计算年龄
                var date = new Date();
                var year = date.getFullYear();
                var birthday_year = parseInt(idcard.substr(6,4));
                var userage= year - birthday_year;
                var datajson = {
                    "id":currentHospitalId,
                    "role": role,
                    "username": username,
                    "idcard": idcard,
                    "mobile": mobile,
                    "email": email,
                    "deptname": deptname,
                    "duty": duty,
                    "tel": tel,
                };

                $.ajax({
                    url: "/F8YLManage/AddUser",
                    contentType: "application/json; charset=utf-8",
                    type: "POST",
                    data: JSON.stringify(datajson),
                    dataType: "json",
                    success: function (retData) {
                        if (retData.code == 0) {
                            //清空
                            $('#user-idcard').val("");
                            $('#user-name').val("");
                            $('#user-email').val("");
                            $('#user-mobile').val("");

                            $('#user-deptname').val("");
                            $('#user-duty').val("");
                            $('#user-tel').val("");
                            $('#user-hospitalname').val("");

                            alert("新增用户成功。");
                            $('#adduser').modal('hide');
                            hospitaluserlist[hospitaluserlist.length]=retData.data;//新增用户是往用户列表里增加一条记录
                            var newuserrole=GetRoleName(retData.data.role);
                            var newuser="<button class='form-control text-left pad-h5 pad-w10 margin-h10 move-user' type='button' id='"+retData.data.id+"'><span class='badge pad-w5 pad-h5 color333'>"+newuserrole+"</span><span>&nbsp;&nbsp;"+retData.data.username+"</span></button>";
                            $('#HospitalUserList').append(newuser);
                            //window.location.href = '/F8YLManage/ManageIndex';
                        }
                        else {
                            alert(retData.message);
                        }
                    },
                    error: function (e) {
                        alert(e);
                    }

                })
            }
        };

        //获取医院明细信息
        function HospitalDetail(id) {
            $(list).each(function(){
                var o = this;
                //alert(o.id);
                currentHospitalId=id;
                if(o.id == id)
                {
                    $('#title').val(o.title);
                    $('#address').val(o.address);
                    $('#contact').val(o.contact);
                    $('#mobile').val(o.mobile);
                    $('#hosp-telphone').val(o.telphone);
                    $('#deptname').val(o.deptname);
                }
            })
        };

