                    //debugger;
                    this.title=$('#title').val();
                    this.address=$('#address').val();
                    this.contact= $('#contact').val();
                    this.mobile=$('#mobile').val();
                    this.telphone=$('#hosp-telphone').val();
                    this.deptname=$('#deptname').val();

                }
            })
            //alert(JSON.stringify(list));
        };

