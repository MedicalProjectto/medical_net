        function GetRoleName(roleid)
        {
            var role="";
            if (roleid=='-10') {
                role='病人'
            }
            else if(roleid=='10') {
                role='医生'
            }
            else if(roleid=='20') {
                role='PI'
            }
            else if(roleid=='80') {
                role='医院管理员'
            }
            else if(roleid=='90') {
                role='后台管理员'
            }
            else {
                role='其它'
            }
            return role;
        }

        //注册新医院
        function createAjaxHospital() {
            var title = $('#hospital_title').val();
            var idcard = $('#hospital_idcard').val();
            var contact = $('#hospital_contact').val();
            var mobile = $('#hospital_mobile').val();

            var address = $('#hospital_address').val();
            var telphone = $('#hospital_telphone').val();
            var deptname = $('#hospital_deptname').val();



            if (!title||!idcard||!contact||!mobile) {
                alert('请填写完成资料！');
                return;
            }

            if (!regPhone.test(mobile)) {
                alert('请输入正确的手机号码！');
                return;
            }
            if (!regIdCard.test(idcard)) {
                alert('请输入正确的身份证号码！');
                return;
            }

            var datajson = {
                "title": title,
                "contact": contact,
                "idcard": idcard,
                "mobile": mobile,

                "address": address,
                "telphone": telphone,
                "deptname": deptname,
            };

            $.ajax({
                url: "/F8YLManage/RegisterHospital",
                contentType: "application/json; charset=utf-8",
                type: "POST",
                data: JSON.stringify(datajson),
                dataType: "json",
                success: function (retData) {
                    if (retData.code == 0) {
                        //清空记录
                        $('#hospital_title').val("");
                        $('#hospital_idcard').val("");
                        $('#hospital_contact').val("");
                        $('#hospital_mobile').val("");
                        $('#hospital_address').val("");
                        $('#hospital_telphone').val("");
                        $('#hospital_deptname').val("");
                        alert("注册医院成功。");
                        $("#add_hospital").modal("hide");
                        var hospitalid=retData.data.hospital.id;
                        var addHtml="<button class='form-control text-left margin-h10 c-hospitals-name' data-toggle='modal' data-target='#edit_hospital' id="+hospitalid+">"+title+"</button>"
                        //alert(addHtml);
                        $("#hostpital-list").append(addHtml);
                        //$("#hostpital-list").on("click","button",function(){
                        //    $('#title').val(retData.data.hospital.title);
                        //    $('#address').val(retData.data.hospital.address);
                        //    $('#contact').val(retData.data.hospital.contact);
                        //    $('#mobile').val(retData.data.hospital.mobile);
                        //});

                        list[list.length]=retData.data.hospital;

                        //window.location.href = '/F8YLManage/ManageIndex';
                    }
                    else {
                        alert(retData.message);
                    }
                },
                error: function (e) {
                    alert(e);
                }

            })

        };

