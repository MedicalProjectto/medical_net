            //保存周期和频次
            $("#savePeriodFreqs, #savecustomperifreq").click(function(){

                var period = "";
                var freqs = "";
                if($(this).attr("id") == "savePeriodFreqs"){
                    period = $("#tpl-period").val();
                    freqs = $("#tpl-freqs").val();
                }else{
                    period = $.trim($("#tpl-cus-period").val());
                    if(period == ""){
                        alert("请输入周期！");
                        return;
                    }
                    if(!/^[0-9]*[1-9][0-9]*$/.test(period)){
                        alert("输入不合法，请输入正整数！");
                        return;
                    }

                    freqs = $.trim($("#tpl-cus-freqs").val());
                    if(freqs == ""){
                        alert("请输入频次！");
                        return;
                    }
                    if(!/^[0-9]*[1-9][0-9]*$/.test(freqs)){
                        alert("输入不合法，请输入正整数！");
                        return;
                    }
                }


                var curEditTpl = null;
                var tplid = $("#tplid").val();
                $.each(templateData, function(i,t){
                    if(t.data.id == tplid){
                        curEditTpl = t;
                        return false;
                    }
                })

                if(curEditTpl == null) return;

                curEditTpl.data.period = period;
                curEditTpl.data.freqs = freqs;

                if($(this).attr("id") == "savePeriodFreqs"){
                    clone('tab10-3');
                }else{
                    clone('tab10-4');
                }

            })

