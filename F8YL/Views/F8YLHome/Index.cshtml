    </div>
    <ul id="navBar">
        <li><a href="javascript:;"><span></span></a></li>
        <li><a href="javascript:;"><span></span></a></li>
        <li><a href="javascript:;"><span></span></a></li>
        <li><a href="javascript:;"><span></span></a></li>
        <li><a href="javascript:;"><span></span></a></li>
    </ul>
    <div class="modal fade" id="login" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" style="max-width:420px;">
            <div class="modal-content">
                <div class="modal-header tc">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        ×
                    </button>
                    <span class="logo">LOGO</span>
                </div>
                <div class="modal-body">
                    <input id="homeMobile" type="text" class="form-control login-tel" placeholder="请输入手机号">
                    <input id="homePassword" type="password" class="form-control login-password margin-h30" placeholder="请输入6-16位密码">
                    <button id="homeLogin" type="button" class="btn btn-primary btn-block" style="height: auto;padding-top: 10px;padding-bottom: 10px;margin-top: 40px;">
                        登录
                    </button>
                    <div class="pad-h10" style="display:none">
                        <a href="javascript:;" class="btn_forget">忘记密码？</a>
                    </div>
                </div>
                <div class="modal-footer" style="text-align: left">
                </div>
            </div>
        </div>
    </div>
    <script src="../js/fullPage.min.js"></script>
    <script type="text/javascript">
        //初始化页面分页
        var runPage;
        runPage = new FullPage({
            id: 'pageContain',
            slideTime: 800,
            effect: {
                transform: {
                    translate: 'Y',
                    scale: [1, 1],
                    rotate: [0, 0]
                },
                opacity: [0, 1]
            },
            mode: 'wheel,touch,nav:navBar',
            easing: 'ease'
        });

        $(function () {
            //返回顶部
            $(".back-top").click(function () {
                $('body,html').animate({ scrollTop: 0 }, 1000);
            })
        })
        document.onkeydown = function () {
            if (event.keyCode == 13) {
                login();
            }
        }
    </script>
</body>
</html>