@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="format-detection" content="telephone=no" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width, height=device-height" />
    <title>医患平台--首页</title>
    <link rel="stylesheet" type="text/css" href="../css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="../css/bootstrap-extend.min.css">
    <link rel="stylesheet" type="text/css" href="../css/base.css">
    <link rel="stylesheet" type="text/css" href="../css/layout.css">
    <link rel="stylesheet" type="text/css" href="../css/style.css">
    <script src="../js/jquery-1.11.3.min.js"></script>
    <script src="../js/jquery.placeholder.min.js"></script>
    <script src="../js/bootstrap.min.js"></script>
    <script type="text/javascript">
        $(function () {
            $('input, textarea').placeholder();
            $('#homeLogin').click(function () {
                login();
            })
        })

        function login() {
            var homeMobile = $.trim($('#homeMobile').val());
            if (homeMobile == "") {
                alert("请输入用户名！");
                return;
            }
            var homePassword = $.trim($('#homePassword').val());
            if (homePassword == "") {
                alert("请输入密码！");
                return;
            }
            $.ajax({
                url: "/F8YLHome/Login?mobile=" + encodeURIComponent(homeMobile) + "&password=" + encodeURIComponent(homePassword),
                type: "GET",
                dataType: "json",
                success: function (retData) {
                    if (retData.code == 0) {
                        window.location.href = '/F8YLProject/ProjectList';
                    }
                    else {
                        alert(retData.message);
                    }
                },
                error: function (e) {

                }
            })
        }
    </script>
    <style>
        html {
            overflow: hidden;
        }

        body {
            width: 100%;
            *cursor: default;
            overflow: hidden;
            font: 16px/1.5 "Microsoft YaHei", Helvetica, STHeiti STXihei, Microsoft JhengHei, Arial;
        }
        .login-box{
            top:40px;
        }
    </style>
</head>
<body style="background:none;">
    <div id="pageContain">
        <div class="page page1 current">
            <div class="contain">
                <div class="margin-h10">
                    <div class="col-xs-6 cCenter">LOGO</div>
                    <div class="col-xs-6 cCenter"><a href="/F8YLHome/Index" style="color:#fff;display:none;">首页</a><button type="button" data-toggle="modal" data-target="#login" class="btn btn-primary btn-xs margin-w20 btn_login">登录</button></div>
                </div>
            </div>
        </div>

        <div class="page page2">
            <div class="contain">
            </div>
        </div>
        <div class="page page3">
            <div class="contain">
            </div>
        </div>
        <div class="page page4">
            <div class="contain">
            </div>
        </div>
        <div class="page page5">
            <div class="contain">
                <div class="container">
                    <div class="col-md-offset-6 col-xs-offset-3" style="margin-top: 300px">
                        <span style="display:inline-block" class="vertical-align-middle">
                            <button class="btn-block" style="background:rgba(1,1,1,0);border: 0;width:190px"><img src="../images/btn-iphone.png"></button>
                            <button class="btn-block" style="background:rgba(1,1,1,0);border: 0;width:190px;margin-top: 30px;"><img src="../images/btn-android.png"></button>
                        </span>
